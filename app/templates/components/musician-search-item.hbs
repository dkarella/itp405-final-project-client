{{#link-to 'dashboard.musician' musician.id tagName='li' class="text-center"}}
  {{#if musician.profilePictureURL}}
    <img src={{musician.profilePictureURL}} class="musician-picture img-circle" alt="musician-picture img-ci"/>
    {{else}}
    <img src="http://placehold.it/150x150" class="musician-picture img-circle" alt="musician-picture"/>
  {{/if}}
  <h2>{{musician.firstName}} {{musician.lastName}}</h2>
{{/link-to}}
